{
    "name": "osioaliu/shopware-queue-monitor",
    "description": "Queue Monitoring for  RabbitMQ Shopware E-Mailjob",
    "type": "shopware-platform-plugin",
    "version": "0.1.1",
    "license": "MIT",
    "require": {
        "shopware/core": "~6.6.0"
    },
    "extra": {
        "shopware-plugin-class": "QueueMonitor\\QueueMonitor",
        "label": {
            "de-DE": "Skeleton plugin",
            "en-GB": "Skeleton plugin"
        }
    },
    "autoload": {
        "psr-4": {
            "QueueMonitor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "QueueMonitor\\Tests\\": "tests/"
        }
    }
}
