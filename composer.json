{
    "name": "aliuosio/shopware-queue-monitor",
    "description": "Queue Monitoring for  RabbitMQ Shopware E-Mailjob",
    "type": "shopware-platform-plugin",
    "version": "0.2.0",
    "license": "MIT",
    "require": {
        "shopware/core": "^v6.6.5.0",
        "symfony/amqp-messenger": "^7.1",
        "php-amqplib/php-amqplib": "^3.7"
    },
    "extra": {
        "shopware-plugin-class": "QueueMonitor\\QueueMonitor",
        "label": {
            "de-DE": "Shopware 6 Queue Monitor",
            "en-GB": "Shopware 6 Queue Monitor"
        }
    },
    "autoload": {
        "psr-4": {
            "QueueMonitor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "QueueMonitor\\Tests\\": "tests/"
        }
    }
}
